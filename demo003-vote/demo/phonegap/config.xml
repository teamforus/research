<?xml version='1.0' encoding='utf-8'?>
<widget id="io.forus.demo.stemapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>StemApp Demo</name>
    <description>
        StemApp Demo
    </description>
    <author email="dev@rminds.nl" href="http://forus.io">
        Forus/Rminds Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icons/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/res/icons/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/res/icons/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/res/icons/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/res/icons/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/res/icons/android/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="www/res/screens/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/res/screens/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/res/screens/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/res/screens/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/res/screens/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/res/screens/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/res/screens/android/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="www/res/screens/android/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="www/res/screens/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="www/res/screens/android/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="www/res/screens/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/res/screens/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="www/res/icons/ios/icon.png" width="57" />
        <icon height="114" platform="ios" src="www/res/icons/ios/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="www/res/icons/ios/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="www/res/icons/ios/icon-40@2x.png" width="80" />
        <icon height="50" platform="ios" src="www/res/icons/ios/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="www/res/icons/ios/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="www/res/icons/ios/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="www/res/icons/ios/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="www/res/icons/ios/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="www/res/icons/ios/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="www/res/icons/ios/icon-72@2x.png" width="144" />
        <icon height="76" platform="ios" src="www/res/icons/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="www/res/icons/ios/icon-76@2x.png" width="152" />
        <icon height="29" platform="ios" src="www/res/icons/ios/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="www/res/icons/ios/icon-small@2x.png" width="58" />
        <icon height="87" platform="ios" src="www/res/icons/ios/icon-small@3x.png" width="87" />
        <splash height="1136" platform="ios" src="www/res/screens/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="www/res/screens/ios/Default-667h.png" width="750" />
        <splash height="2208" platform="ios" src="www/res/screens/ios/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="www/res/screens/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="www/res/screens/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" platform="ios" src="www/res/screens/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="www/res/screens/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="www/res/screens/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="960" platform="ios" src="www/res/screens/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="www/res/screens/ios/Default~iphone.png" width="320" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="demomeapp://*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <config-file parent="LSApplicationQueriesSchemes" platform="ios" target="*-Info.plist">
        <array>
            <string>demomeapp</string>
        </array>
    </config-file>
    <engine name="browser" spec="~5.0.3" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-battery-status" spec="^1.2.5" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1" />
    <plugin name="cordova-plugin-media-capture" spec="^1.4.3" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.4" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-globalization" spec="^1.0.9" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.4" />
    <plugin name="cordova-plugin-vibration" spec="^2.1.6" />
    <plugin name="cordova-plugin-customurlscheme" spec="^4.3.0">
        <variable name="URL_SCHEME" value="demostemapp" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-add-swift-support" spec="^1.7.1" />
    <plugin name="cordova-plugin-appavailability" spec="^0.4.2" />
    <engine name="android" spec="~6.4.0" />
</widget>
